if game.PlaceId == 6085586197 and _G.VCheck2 == "v1.4" then
--nothing
else
    if game.PlaceId == 6085586197 then
    local function callback(Text)
        if Text == "Re-Execute" then
            loadstring(game:HttpGet("https://raw.githubusercontent.com/HDTerebi/Project-Hero/main/PH.lua"))()
			elseif Text == "ReJoin" then
			local tpservice= game:GetService("TeleportService")
local plr = game.Players.LocalPlayer

tpservice:Teleport(game.PlaceId, plr)
        end
    end
    
    local bf = Instance.new("BindableFunction")
    bf.OnInvoke = callback
game.StarterGui:SetCore("SendNotification", {
Title = "Project Hero Gui";
Text = "New Version Of The Gui Detected, Please Re-Execute The Script Or Rejoin",
Icon = "rbxassetid://6753829832";
Duration = 8;
Button1 = "Re-Execute";
Button2 = "ReJoin";
Callback = bf
})
end
end
